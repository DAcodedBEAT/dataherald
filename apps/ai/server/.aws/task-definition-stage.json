{
    "containerDefinitions": [
        {
            "name": "ai-engine-stage",
            "image": "422486916789.dkr.ecr.us-east-1.amazonaws.com/ai-engine-stage:latest",
            "cpu": 1024,
            "memory": 3072,
            "portMappings": [
                {
                    "name": "ai-engine-stage-3001-tcp",
                    "containerPort": 3001,
                    "hostPort": 3001,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [
                {
                    "value": "arn:aws:s3:::ai-engine-stage-environment-variables/.env",
                    "type": "s3"
                }
            ],
            "command": [
                "sh",
                "-c",
                "uvicorn dataherald.app:app --host 0.0.0.0 --port $CORE_PORT"
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/ai-engine-stage",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        },
        {
            "name": "ai-server-stage",
            "image": "422486916789.dkr.ecr.us-east-1.amazonaws.com/ai-server-stage:latest",
            "cpu": 1024,
            "memory": 3072,
            "portMappings": [
                {
                    "name": "ai-server-stage-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [
                {
                    "value": "arn:aws:s3:::ai-server-stage-environment-variables/.env",
                    "type": "s3"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/ai-server-stage",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "ai-backend-stage",
    "taskRoleArn": "arn:aws:iam::422486916789:role/ecsk2TaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::422486916789:role/ecsk2TaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "2048",
    "memory": "6144",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}