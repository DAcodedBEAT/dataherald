{
  "containerDefinitions": [
    {
      "name": "ai-engine-prod",
      "image": "422486916789.dkr.ecr.us-east-1.amazonaws.com/ai-engine-prod:latest",
      "cpu": 0,
      "portMappings": [
        {
          "name": "ai-engine-prod-3001-tcp",
          "containerPort": 3001,
          "hostPort": 3001,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [
        {
          "value": "arn:aws:s3:::ai-engine-prod-environment-variables/.env",
          "type": "s3"
        }
      ],
      "command": [
          "sh",
          "-c",
          "uvicorn dataherald.app:app --host 0.0.0.0 --port $CORE_PORT"
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/ai-engine-prod",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    },
    {
      "name": "ai-server-prod",
      "image": "422486916789.dkr.ecr.us-east-1.amazonaws.com/ai-server-prod:latest",
      "cpu": 0,
      "portMappings": [
        {
          "name": "ai-server-prod-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [
        {
          "value": "arn:aws:s3:::ai-server-prod-environment-variables/.env",
          "type": "s3"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/ai-server-prod",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "family": "ai-backend-prod",
  "taskRoleArn": "arn:aws:iam::422486916789:role/ecsk2TaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::422486916789:role/ecsk2TaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "2048",
  "memory": "6144",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "tags": []
}
